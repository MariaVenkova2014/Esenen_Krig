#include <iostream>
using namespace std;

int main() {
    int a[3][3];
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            cin >> a[i][j];
        }
    }

    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            if (a[i][j] == 9) {
                cout << 9;
            } else {
                int bombs = 0;
                for (int di = -1; di <= 1; di++) {
                    for (int dj = -1; dj <= 1; dj++) {
                        if (di == 0 && dj == 0) continue;
                        int ni = i + di, nj = j + dj;
                        if (ni >= 0 && ni < 3 && nj >= 0 && nj < 3) {
                            if (a[ni][nj] == 9) bombs++;
                        }
                    }
                }
                cout << bombs;
            }
            if (j < 2) cout << " ";
        }
        cout << "\n";
    }

    return 0;
}
