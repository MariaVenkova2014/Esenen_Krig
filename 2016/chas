#include <iostream>
using namespace std;

int main() {
    int t1, m1, t2, m2;
    cin >> t1 >> m1 >> t2 >> m2;

    int startMinutes = t1 * 60 + m1;
    int endMinutes = t2 * 60 + m2;

    int totalMinutes = endMinutes - startMinutes;
    if (totalMinutes < 0) totalMinutes += 24 * 60;

    int laps = totalMinutes / 30;

    cout << totalMinutes << " " << laps << endl;

    return 0;
}
