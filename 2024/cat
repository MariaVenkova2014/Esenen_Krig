#include <iostream>
using namespace std;

int main() {
    int h, m, n;
    cin >> h >> m >> n;

    int totalMinutes = h * 60 + m + n;
    totalMinutes %= 24 * 60;

    h = totalMinutes / 60;
    m = totalMinutes % 60;

    if (h < 10) cout << "0" << h;
    else cout << h;
    cout << ":";
    if (m < 10) cout << "0" << m;
    else cout << m;
    cout << endl;
}
