#include <iostream>
#include <string>
using namespace std;

int main() {
    string c1, c2, c3, c4;
    cin >> c1 >> c2 >> c3 >> c4;

    // слагаме ги в масив от низове
    string arr[4] = {c1, c2, c3, c4};

    // ръчно сортиране по x+y > y+x
    for (int i = 0; i < 4; i++) {
        for (int j = i + 1; j < 4; j++) {
            if (arr[i] + arr[j] < arr[j] + arr[i]) {
                string temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }

    // ако първият е "0", значи всички са нули → печатаме само 0
    if (arr[0] == "0") {
        cout << 0;
    } else {
        for (int i = 0; i < 4; i++) {
            cout << arr[i];
        }
    }

    cout << endl;
}
