#include <iostream>
using namespace std;

int main() {
    long long a, b, c, d;
    if (!(cin >> a >> b >> c >> d)) return 0;

    long long mn = a;
    if (b < mn) mn = b;
    if (c < mn) mn = c;
    if (d < mn) mn = d;

    long long sum = a + b + c + d;
    cout << (sum - mn + 1) << endl;
    return 0;
}
