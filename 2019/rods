#include <iostream>
using namespace std;

int main() {
    int a1, a2, a3, b1, b2, b3, L;
    cin >> a1 >> a2 >> a3 >> b1 >> b2 >> b3 >> L;

    int minCount = 1e9; // голямо число за минимум

    for (int x1 = 0; x1 <= b1; x1++) {
        for (int x2 = 0; x2 <= b2; x2++) {
            for (int x3 = 0; x3 <= b3; x3++) {
                int length = x1 * a1 + x2 * a2 + x3 * a3;
                if (length >= L) {
                    int sticks = x1 + x2 + x3;
                    if (sticks < minCount) minCount = sticks;
                }
            }
        }
    }

    if (minCount == 1e9)
        cout << 0;
    else
        cout << minCount;

    return 0;
}
