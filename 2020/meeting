#include <iostream>
using namespace std;

int main() {
    int l1, r1, l2, r2, k;
    cin >> l1 >> r1 >> l2 >> r2 >> k;

    int start = max(l1, l2);
    int end = min(r1, r2);

    if (start > end) {
        cout << 0 << endl;
        return 0;
    }

    int duration = end - start + 1;

    if (k >= start && k <= end) {
        duration--;
    }

    cout << duration << endl;
}
