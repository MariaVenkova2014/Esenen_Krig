#include <iostream>
using namespace std;

int main() {
    int a[4];
    for (int i = 0; i < 4; i++) cin >> a[i];

    int countEmpty = 0;
    int emptyIndex = -1;
    int missingNums[2], mCount = 0;

    bool present[5] = {false}; // индекси 1..4

    for (int i = 0; i < 4; i++) {
        if (a[i] == 0) {
            countEmpty++;
            emptyIndex = i + 1; // кутийките са от 1 до 4
        } else {
            present[a[i]] = true;
        }
    }

    for (int num = 1; num <= 4; num++) {
        if (!present[num]) {
            missingNums[mCount] = num;
            mCount++;
        }
    }

    if (countEmpty == 1) {
        cout << emptyIndex << " " << missingNums[0];
    } else if (countEmpty == 2) {
        if (missingNums[0] < missingNums[1])
            cout << missingNums[0] << " " << missingNums[1];
        else
            cout << missingNums[1] << " " << missingNums[0];
    } else {
        cout << a[0] << " " << a[1];
    }

    return 0;
}
